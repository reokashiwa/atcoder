#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

int main() {
  int N;
  cin >> N;
  if ((N < 1) || (N > 10e5))
    exit(1);
  vector<vector<int>> A(N, vector<int>);
  rep (i, 3 * N) {
    int a;
    cin >> a;
    if ((a < 1) || (a > N))
      exit(1);
    A[a].push_back(i);
  }
}
