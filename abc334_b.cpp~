#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

int main() {
  long long int A, M, L, R;
  cin >> A >> M >> L >> R;
  if ((A < -1 * 10e18) || (A > 10e18))
    exit(1);
  if ((M < 1) || (M > 10e9))
    exit(1);
  if ((L < -1 * 10e18) || (L > 10e18))
    exit(1);
  if ((R < -1 * 10e18) || (R > 10e18))
    exit(1);
  if (L > R)
    exit(1);
}
