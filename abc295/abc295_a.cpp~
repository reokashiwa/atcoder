#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

int main() {
  int N;
  cin >> N;
  bool flag = false;
  rep (i, N) {
    string W;
    cin >> W;
    switch(W) {
    case "and":
      flag = true;
      break;

    case "not":
      flag = true;
      break;

    case "that":
      flag = true;
      break;

    case "the":
      flag = true;
      break;

    case "you":
      flag = true;
      break;

    case default:
      break;
    }
  }
}
