#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

int main() {
  int N, M, L;
  cin >> N;
  vector<int> A(N);
  rep (i, N) {
    int a;
    cin >> a;
    if ((a < 0) || (a > 10e8))
      exit(1);
    A[i] = a;
  }
  cin >> M;
  vector<int> B(M);
  rep (i, M) {
    int b;
    cin >> b;
    if ((b < 0) || (b > 10e8))
      exit(1);
    B[i] = b;
  }
  cin >> L;
  vector<int> C(L);
  rep (i, L) {
    int c;
    cin >> c;
    if ((c < 0) || (c > 10e8))
      exit(1);
    C[i] = c;
  }
}
