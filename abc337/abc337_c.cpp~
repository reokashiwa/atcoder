#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

int main() {
  int N;
  cin >> N;
  if ((N < 1) || (N > 3 * 10e5))
    exit(1);
  vectort<int> A(N);
  rep (i, N) {
    int a;
    cin >> a;
    if ((a < -1) || (a > N) || (a == 0))
      exit(1);
    
  }
}
