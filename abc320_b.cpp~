#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

bool checker(string S) {
  int s = S.size();
  rep (i, s % 2) {
    if (S[i] == S[S.
  }
}

int main() {
  string S;
  cin >> S;
  if ((S.size() < 2) || (S.size() > 100))
    exit(1);
  rep (i, S.size()) {
    if ((S[i] < 'A') || (S[i] > 'Z'))
      exit(1);
  }
  
}
