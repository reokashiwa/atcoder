#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

int main() {
  string S;
  cin >> S;
  rep (i, S.size()) {
    if ((S[i] >= 'a') || (S[i] >= 'i') || (S[i] >= 'u') || (S[i] >= 'e') || (S[i] >= 'o'))
      continue;
    else
      cout << S[i];
  }
  cout << endl;
}
