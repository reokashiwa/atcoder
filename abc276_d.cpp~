#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

int main() {
  int N;
  cin >> N;
  if ((N < 1) || (N > 50))
    exit(1);
  int X;
  cin >> X;
  if ((X < 1) || (X > 10e4))
    exit(1);
  vector<int> A(N);
  vector<int> B(N);
  int total = 0;
  rep (i, N) {
    int a, b;
    cin >> a >> b;
    if ((a < 1) || (a > 100))
      exit(1);
    if ((b < 1) || (b > 50))
      exit(1);
    A[i] = a;
    B[i] = b;
    total = total + a * b;
  }

  
  
}
