#include <bits/stdc++.h>
using namespace std;
#define rep(i, n) for (int i = 0; i < (int)(n); i++)

int main() {
  int N;
  cin >> N;
  if ((N < 2) || (N > 100))
    exit(1);

  rep (i, N) {
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    if ((a < 0) || (a > 100))
      exit(1);
    if ((b < 0) || (b > 100))
      exit(1);
    if (a >= b)
      exit(1);
    if ((c < 0) || (c > 100))
      exit(1);
    if ((d < 0) || (d > 100))
      exit(1);
    if (c >= d)
      exit(1);
    
  }
}
